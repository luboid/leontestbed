			<xs:element name="PlusMnsTp" type="PlusMinusTypeCode1" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GroupHeader1">
		<xs:sequence>
			<xs:element name="MsgId" type="Max35Text"/>
