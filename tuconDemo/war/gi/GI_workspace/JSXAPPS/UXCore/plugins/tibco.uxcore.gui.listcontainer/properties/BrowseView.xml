<data jsxid="jsxroot">

    <record group="object" include="master.xml"/>

    <record group="1" jsxid="browseView" jsxtext="Browse View">
        <record jsxid="panellabels" jsxtext="Panel Labels" jsxtip="Sets labels of browse panels, it's a comma-splitted string, for example, Servers,Machine,Nodes"
            docsetter="setPanelLabels" docgetter="getPanelLabels" 
            eval="0" jsxexecute="objJSX.setPanelLabels(vntValue, true);" jsxmask="jsxtext"/>
        <record jsxid="panelids" jsxtext="Panel IDs" jsxtip="Sets ids of browse panels, it's a comma-splitted string, for example, servers,machine,nodes, those ids is used in API of Browse and ListContainer to identify which panel you want to update, set data or make a selection "
            docsetter="setPanelIds" docgetter="getPanelIds"
            eval="0" jsxexecute="objJSX.setPanelIds(vntValue);" jsxmask="jsxtext"/>
        <record jsxid="jsxpanelwidths" jsxtext="Panel Widths"
            eval="0"
            jsxtip="Sets a comma-separated group of values to describe how width each panel is. For example, &lt;code&gt;10%,25,*&lt;/code&gt; or &lt;code&gt;200,*,32,8.&lt;/code&gt;"
            docgetter="getPanelWidths" docsetter="setPanelWidths"
            docdefault="&lt;code&gt;*&lt;/code&gt;"
            jsxexecute="objJSX.setPanelWidths(vntValue, true);"
            jsxmask="jsxtext"/>
    </record>

</data>
