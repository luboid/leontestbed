<data jsxid="jsxroot">

    <record group="object" include="master.xml"/>

    <record group="1" jsxid="uxcoreFilter" jsxtext="Filter">
        <record jsxid="jsxfilterboxwidth" jsxtext="FilterBox Width" jsxtip="Sets width of filter selectbox"
            docsetter="setFilterBoxWidth" docgetter="getFilterBoxWidth" 
            eval="0" jsxexecute="objJSX.setFilterBoxWidth(vntValue);" jsxmask="jsxtext"/>
        <record jsxid="jsxattributes" jsxtext="Attributes" jsxtip="Sets the Filter Attribute(s)"
            docsetter="setSearchAttributes" docgetter="getSearchAttributes"
            eval="0" jsxexecute="objJSX.setSearchAttributes(vntValue);" jsxmask="jsxtext"/>
        <record docdefault="&lt;code&gt;true (jsx3.Boolean.TRUE)&lt;/code&gt;" docgetter="getControlVeto" docsetter="setControlVeto"
            eval="1" jsxexecute="objJSX.setControlVeto(vntValue);" jsxid="jsxcontrolVeto" jsxmask="jsxselect"
            jsxtext="Control veto"  jsxtip="To decide whether the component control the veto of any action in the FilterSearch">
                <enum jsxid="jsx3.Boolean.TRUE" jsxtext="True"/>
                <enum jsxid="jsx3.Boolean.FALSE" jsxtext="False"/>
        </record>
    </record>

    <record group="css" include="master.xml"/>

</data>
