<data jsxid="jsxroot">
	<record include="master.xml" group="object"/>
	<record include="master.xml" group="position"/>

    <record jsxid="uxcoreTitleBox" group="1" jsxtext="TitleBox">
        <record jsxid="title" jsxtext="Title"
            jsxtip="Sets the title for the TitleBox."
            docgetter="getTitle" docsetter="setTitle"
            eval="0"
            jsxexecute="objJSX.setTitle(vntValue);"
            jsxmask="jsxtext"/>
        <record jsxid="description" jsxtext="Description"
            jsxtip="Sets the description for the TitleBox"
            docgetter="getDescription" docsetter="setDescription"
            eval="0"
            jsxexecute="objJSX.setDescription(vntValue);"
            jsxmask="jsxtext"/>
        <record jsxid="titleFontSize" jsxtext="Title Font Size"
            jsxtip="Sets the font size for the Title"
            docgetter="getTitleFontSize" docsetter="setTitleFontSize"
            eval="0"
            jsxexecute="objJSX.setTitleFontSize(vntValue);"
            jsxmask="jsxtext"/>
    </record>
	
