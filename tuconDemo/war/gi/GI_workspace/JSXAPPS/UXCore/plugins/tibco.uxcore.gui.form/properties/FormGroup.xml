            eval="0"
            jsxexecute="objJSX.setErrorText(vntValue, true);"
            jsxmask="jsxtext"/> -->
        <record jsxid="instructionstext" jsxtext="Instructions Text"
            jsxtip="Sets the instructions text for the form group"
            docgetter="getInstructionsText" docsetter="setInstructionsText"
            eval="0"
            jsxexecute="objJSX.setInstructionsText(vntValue, true);"
            jsxmask="jsxtext"/>
        <record docdefault="&lt;code&gt;true (jsx3.Boolean.TRUE)&lt;/code&gt;" docgetter="getShowGlobalErrors"
                docsetter="setShowGlobalErrors" eval="1" jsxid="showGlobalErrors" jsxmask="jsxselect" jsxtext="Show Global Errors"
