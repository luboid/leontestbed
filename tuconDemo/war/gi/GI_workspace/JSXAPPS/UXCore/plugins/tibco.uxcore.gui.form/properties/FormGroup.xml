            <enum jsxid="jsx3.Boolean.TRUE" jsxtext="True"/>
            <enum jsxid="jsx3.Boolean.FALSE" jsxtext="False"/>
        </record>
        <record docdefault="&lt;code&gt;true (jsx3.Boolean.TRUE)&lt;/code&gt;" docgetter="getToggleEditModeOnLoseFocus"
