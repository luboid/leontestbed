            <enum jsxid="jsx3.Boolean.TRUE" jsxtext="True"/>
            <enum jsxid="jsx3.Boolean.FALSE" jsxtext="False"/>
        </record>
        <record docdefault="&lt;code&gt;true (jsx3.Boolean.TRUE)&lt;/code&gt;" docgetter="getConfirmEditsOnLoseFocus"
                docsetter="setConfirmEditsOnLoseFocus" eval="1" jsxid="confirmEditsOnLoseFocus" jsxmask="jsxselect" jsxtext="Confirm Edits On Lose Focus"
